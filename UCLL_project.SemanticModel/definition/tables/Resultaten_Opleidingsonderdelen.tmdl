	measure Studie-efficiÃ«ntie = ```
			
			VAR CreditsTable =
			    VALUES('Resultaten_Opleidingsonderdelen'[OPO_ID])
			VAR Earned =
			    SUMX(
			        CreditsTable,
			        VAR GradeP1 = CALCULATE(
			                            MAX('Resultaten_Opleidingsonderdelen'[Resultaat]),
			                            'Resultaten_Opleidingsonderdelen'[Periode] = 1
			                        )
			        VAR GradeP2 = CALCULATE(
			                            MAX('Resultaten_Opleidingsonderdelen'[Resultaat]),
			                            'Resultaten_Opleidingsonderdelen'[Periode] = 2
			                        )
			        VAR BestGrade = IF(NOT(ISBLANK(GradeP2)) && GradeP2 > GradeP1, GradeP2, GradeP1)
			        VAR Invested = MAX('Resultaten_Opleidingsonderdelen'[InvestedECTS])
			        RETURN IF(BestGrade >= 10, Invested, 0)
			    )
			VAR Invested =
			    SUMX(
			        CreditsTable,
			        MAX('Resultaten_Opleidingsonderdelen'[InvestedECTS])
			    )
			RETURN DIVIDE(Earned, Invested, 0)
			
