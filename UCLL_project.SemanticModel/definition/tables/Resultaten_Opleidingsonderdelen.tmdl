	column Effective_Credits = ```
			
			VAR P2 =
			    CALCULATE(
			        MAX('Resultaten_Opleidingsonderdelen'[ECTS]),
			        'Resultaten_Opleidingsonderdelen'[Periode] = 2,
			        ALLEXCEPT(
			            'Resultaten_Opleidingsonderdelen',
			            'Resultaten_Opleidingsonderdelen'[Student_ID],
			            'Resultaten_Opleidingsonderdelen'[OPO_ID],
			            'Resultaten_Opleidingsonderdelen'[Fase_Semester]
			        )
			    )
			VAR P1 =
			    CALCULATE(
			        MAX('Resultaten_Opleidingsonderdelen'[ECTS]),
			        'Resultaten_Opleidingsonderdelen'[Periode] = 1,
			        ALLEXCEPT(
			            'Resultaten_Opleidingsonderdelen',
			            'Resultaten_Opleidingsonderdelen'[Student_ID],
			            'Resultaten_Opleidingsonderdelen'[OPO_ID],
			            'Resultaten_Opleidingsonderdelen'[Fase_Semester]
			        )
			    )
			VAR FinalCredits = IF(NOT(ISBLANK(P2)), P2, P1)
			RETURN IF(FinalCredits > 0 || 'Resultaten_Opleidingsonderdelen'[Vrijgesteld], FinalCredits, 0)
			
